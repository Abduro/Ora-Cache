<Project DefaultTargets="Build" ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup><!--Node names must be unique and do not coincident with environment variable names neither system nor custom ones!-->
    <Shared Condition="'$(Shared)'==''">..\..\..\ms.win\sys.shared\</Shared>
    <Error Condition="'$(Error)'==''">..\..\..\ms.win\sys.err.core\</Error>
    <Color Condition="'$(Color)'==''">..\..\..\ms.win\uix\draw\color\</Color>
    <Geo-2D Condition="'$(Geo-2D)'==''">..\..\..\ms.win\uix\geometry\2D\</Geo-2D>
    <DirectX Condition="'$(DirectX)'==''">..\..\..\ms.win\uix\draw\rnd.direct.x\</DirectX><!--This library is being tested!-->
    <Render Condition="'$(Render)'==''">..\..\..\ms.win\uix\draw\render\</Render>
    <Runner Condition="'$(Runner)'==''">..\..\..\ms.win\runnable\</Runner>
    <ThisLib Condition="'$(ThisLib)'==''">..\..\..\ms.win.cppunit.test\</ThisLib>
  </PropertyGroup>
  <PropertyGroup>
    <Root>$(ThisLib)</Root>
  </PropertyGroup>
  <PropertyGroup><!--this nodes must be specified directly in paths of project files!-->
    <_0>$(Root)interface\</_0>
    <_1>$(Root)src\</_1>
    <_2>$(Shared)interface\</_2>
    <_3>$(Color)interface\</_3>
    <_4>$(_3)named\</_4>
    <_5>$(Geo-2D)interface\</_5>
    <_6>$(DirectX)interface\</_6><!--This library interface is being tested!-->
    <_7>$(Error)interface\</_7>
    <_8>$(_8)seh\</_8>
    <_9>$(Runner)interface\</_9>
    <_a>$(Render)interface\</_a>
  </PropertyGroup>
  <PropertyGroup> <!--*important*::it does *not* work in c++ project file if it is declared there!-->
    <IncludePath>$(_0);$(_1);$(_2);$(_3);$(_4);$(_5);$(_6);$(_7);$(_8);$(_9);$(_a);$(IncludePath)</IncludePath>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup />
</Project>