<Project DefaultTargets="Build" ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup><!--Node names must be unique and do not coincident with environment variable names neither system nor custom ones!-->
    <Shared Condition="'$(Shared)'==''">..\..\..\ms.win\sys.shared\</Shared>
    <Error Condition="'$(Error)'==''">..\..\..\ms.win\sys.err.core\</Error>
    <Geo-2d Condition="'$(Geo-2d)'==''">..\..\..\ms.win\uix\geometry\2D\</Geo-2d>
    <Color Condition="'$(Color)'==''">..\..\..\ms.win\uix\draw\color\</Color>
    <Render Condition="'$(Render)'==''">..\..\..\ms.win\uix\draw\render\</Render>
    <Win-pop Condition="'$(Win-pop)'==''">..\..\..\ms.win\uix\popup\</Win-pop>
    <ThisExe Condition="'$(ThisExe)'==''">..\..\..\ms.win.gui\</ThisExe>
  </PropertyGroup>
  <PropertyGroup>
    <Root>$(ThisExe)</Root>
  </PropertyGroup>
  <PropertyGroup><!--this nodes must be specified directly in paths of project files!-->
    <_0>$(Root)interface\</_0>
    <_1>$(Root)src\</_1>
    <_2>$(_1)resources\</_2>
    <_3>$(_2)icons\</_3>
    <_4>$(Shared)interface\</_4>
    <_5>$(Error)interface\</_5>
    <_6>$(_5)seh\</_6>
    <_7>$(Geo-2d)interface\</_7>
    <_8>$(Color)interface\</_8>
    <_9>$(Render)interface\</_9>
    <_a>$(Win-pop)interface\</_a>
    <_b>$(_a)msg_handlers\</_b>
    <_c>$(_1)render\</_c>          <!--this is the folder for including render interface wrappers, especially direct_x for this time;-->
    <_d>$(_9)direct_x\</_d>
    <_e>$(_1)themes\</_e>
  </PropertyGroup>
  <PropertyGroup>
    <IncludePath>$(_0);$(_1);$(_2);$(_3);$(_4);$(_5);$(_6);$(_7);$(_8);$(_9);$(_a);$(_b);$(_c);$(_d);$(_e);$(IncludePath)</IncludePath>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup />
</Project>